{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "TinyFn",
  "colors": {
    "primary": "#00ff88",
    "light": "#00ff88",
    "dark": "#00cc6a"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication",
              "mcp-integration"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/rate-limits",
              "guides/error-handling"
            ]
          }
        ]
      },
      {
        "tab": "MCP",
        "groups": [
          {
            "group": "MCP Integration",
            "pages": [
              "mcp/overview",
              "mcp/categories"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "https://api.tinyfn.io/openapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://tinyfn.io/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@tinyfn.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://tinyfn.io/#signup"
    }
  },
  "api": {
    "baseUrl": "https://api.tinyfn.io",
    "playground": {
      "mode": "simple"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tinyfn",
      "github": "https://github.com/tinyfn",
      "discord": "https://discord.gg/tinyfn"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "search": {
    "prompt": "Search endpoints..."
  }
}
